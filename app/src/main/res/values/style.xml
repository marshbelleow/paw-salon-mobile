<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="ButtonBase" parent="Widget.MaterialComponents.Button">
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:gravity">center</item>
        <item name="android:elevation">15dp</item>
    </style>


    <style name="RoundedCorners20">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">20dp</item>
    </style>


    <style name="RoundedCorners50">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">50dp</item>
    </style>

    <style name="roundimageview" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">25dp</item>
        <item name="strokeColor">@color/mama_mo_blue_darker</item>
        <item name="strokeWidth">5dp</item>

    </style>


</resources>
